{
  "name": "web",
  "type": "module",
  "private": true,
  "imports": {
    "#internal/nuxt/paths": "./internal/nuxt-paths.mjs"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest",
    "test:e2e": "rm -rf .nuxt .output && pnpm exec nuxt prepare && NUXT_PUBLIC_SUPABASE_URL=http://localhost:54321 NUXT_PUBLIC_SUPABASE_ANON_KEY=test-anon-key pnpm build && sh ./scripts/run-e2e.sh run",
    "test:e2e:open": "rm -rf .nuxt .output && pnpm exec nuxt prepare && NUXT_PUBLIC_SUPABASE_URL=http://localhost:54321 NUXT_PUBLIC_SUPABASE_ANON_KEY=test-anon-key pnpm build && sh ./scripts/run-e2e.sh open"
  },
  "dependencies": {
    "@fontsource/atkinson-hyperlegible": "^5.2.8",
    "@fontsource/fraunces": "^5.2.5",
    "@fontsource/ibm-plex-sans": "^5.2.6",
    "@nuxtjs/supabase": "^1.6.0",
    "@primeuix/themes": "^2.0.3",
    "@primevue/nuxt-module": "^4.3.3",
    "@supabase/supabase-js": "^2.56.1",
    "chart.js": "^4.5.1",
    "markdown-it": "^14.1.1",
    "nuxt": "^4.2.2",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.3",
    "sanitize-html": "^2.17.0",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@nuxtjs/tailwindcss": "^6.12.2",
    "@tailwindcss/typography": "^0.5.16",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/compiler-sfc": "^3.5.26",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^15.8.1",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.3.0",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16",
    "vue-eslint-parser": "^10.2.0"
  }
}
